{
  "name": "yaren-app-portable",
  "version": "1.0.1",
  "description": "牙人委托发布与查看应用（便携版）",
  "main": "main-compat-fixed.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron .",
    "clean-dist": "rimraf portable-dist-fixed",
    "build": "npm run clean-dist && electron-builder --config fixed-portable-config.js",
    "build-unpacked": "npm run clean-dist && electron-builder --config fixed-portable-config.js --dir",
    "release": "npm run build && electron-builder --publish never --config fixed-portable-config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "fayufm",
  "license": "ISC",
  "devDependencies": {
    "electron": "^36.0.0",
    "electron-builder": "^26.0.12",
    "rimraf": "^5.0.5"
  },
  "dependencies": {
    "axios": "^1.9.0"
  }
} 